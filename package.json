{
  "name": "tree-view-panes",
  "main": "./lib/tree-view-panes.js",
  "version": "0.2.1",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "description": "Show list of open panes and files on the top of the tree view",
  "scripts": {
    "lint": "eslint lib --fix"
  },
  "keywords": [
    "tree-view",
    "open",
    "panes",
    "files"
  ],
  "repository": "https://github.com/susisu/tree-view-panes",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "fs-plus": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^3.15.0"
  },
  "activationCommands": {},
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "1.0.0": "consumeFileIcons"
      }
    }
  },
  "deserializers": {
    "tree-view-panes/DockItem": "getDockItem"
  },
  "configSchema": {
    "showPendingItems": {
      "order"      : 1,
      "type"       : "boolean",
      "default"    : true,
      "description": "Show pending items on the tree view."
    },
    "showGitStatus": {
      "order"      : 2,
      "type"       : "boolean",
      "default"    : true,
      "description": "Show Git statuses of files."
    }
  }
}
